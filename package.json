{
  "name": "veoci-client",
  "packageManager": "yarn@3.3.0",
  "version": "4.2.0",
  "description": "Shared Veoci client monorepo",
  "main": ".eslintrc.js",
  "private": true,
  "workspaces": {
    "packages": [
      "veoci-web",
      "veoci-js",
      "veoci-mobile"
    ]
  },
  "scripts": {
    "test": "yarn lint",
    "lint": "node --max_old_space_size=4096 ./node_modules/eslint/bin/eslint.js --ext .js,.vue veoci-shared veoci-web/src veoci-web/test veoci-web/build veoci-shared",
    "stylelint": "node --max_old_space_size=4096 ./node_modules/stylelint/bin/stylelint.js **/*.scss **/*.css **/*.vue --config .stylelintrc",
    "i18n": "node veoci-shared/scripts/add-translation-key.js",
    "js:lint": "tslint -p tsconfig.json -t codeFrame 'veoci-js/src/**/*.ts' 'veoci-js/test/**/*.ts'",
    "js:start": "yarn workspace veoci-js dev",
    "js:build": "yarn workspace veoci-js build",
    "js:build:docs": "yarn workspace veoci-js build:docs",
    "web:unit": "yarn workspace veoci-web unit",
    "web:jest": "yarn workspace veoci-web test:jest",
    "web:jest:ci": "yarn workspace veoci-web test:jest:ci",
    "web:jest:watch": "yarn workspace veoci-web test:jest:watch",
    "web:jest:coverage": "yarn workspace veoci-web test:jest:coverage",
    "web:unit:watch": "yarn workspace veoci-web run unit:watch",
    "web:unit:debug": "yarn workspace veoci-web run unit:debug",
    "web:start:local": "yarn workspace veoci-web dev:local",
    "web:start:dev": "yarn workspace veoci-web dev",
    "web:start:stage": "yarn workspace veoci-web dev:stage",
    "web:start:stageEU": "yarn workspace veoci-web dev:stageEU",
    "web:start:prod": "yarn workspace veoci-web dev:prod",
    "web:dev": "node scripts/web-dev.js",
    "web:dev:pwa": "ENABLE_DEV_PWA=true node scripts/web-dev.js",
    "web:build:stage": "yarn workspace veoci-web triggerStageBuild",
    "web:build": "yarn workspace veoci-web triggerProdBuild",
    "web:bumpVersion": "yarn workspace veoci-web bumpProdVersion",
    "web:mergeMaster": "yarn workspace veoci-web mergeMaster",
    "web:mergeStageStash": "yarn workspace veoci-web mergeStageStash",
    "web:mergeStageIntoStash": "yarn workspace veoci-web mergeStageIntoStash",
    "mobile:start": "yarn workspace veoci-mobile dev",
    "mobile:dev": "concurrently -r \" yarn js:start\" \" yarn mobile:start\"",
    "mobile:build:stage": "node veoci-mobile/build/triggerStageBuild",
    "mobile:build": "node veoci-mobile/build/triggerProdBuild",
    "mobile:run:android": "cd veoci-mobile && quasar dev -m android --debug -- --device",
    "mobile:quasar:android:debug": "cd veoci-mobile && RELEASE_STAGE=DEV quasar build -m android -- -- --device",
    "mobile:quasar:ios:debug": "cd veoci-mobile && RELEASE_STAGE=DEV quasar build -m ios --debug -- --device",
    "mobile:quasar:android": "cd veoci-mobile && yarn build:android --debug",
    "mobile:quasar:ios": "cd veoci-mobile && yarn build:ios",
    "mobile:quasar:android:prod": "cd veoci-mobile && yarn build:android:prod",
    "mobile:quasar:ios:prod": "cd veoci-mobile && yarn build:ios:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreywallSoftware/veoci-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GreywallSoftware/veoci-client/issues"
  },
  "homepage": "https://github.com/GreywallSoftware/veoci-client#readme",
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/eslint-parser": "^7.22.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.22.5",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.7",
    "@babel/plugin-transform-spread": "^7.22.5",
    "@babel/preset-env": "^7.22.7",
    "@babel/register": "^7.22.5",
    "@babel/runtime": "^7.22.6",
    "@google-cloud/translate": "^7.0.5",
    "@intlify/vue-i18n-loader": "next",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "chai": "^4.1.2",
    "color": "^4.2.3",
    "colors": "^1.4.0",
    "concurrently": "^7.4.0",
    "core-js": "^3.25.2",
    "eslint": "8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.11.0",
    "eslint-plugin-vuetify": "^2.0.1",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "prompt": "^1.0.0",
    "string.prototype.replaceall": "^1.0.6",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^5.1.6",
    "veoci-web": "workspace:^",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "workspace": "^0.0.1-preview.1"
  },
  "dependencies": {
    "@bugsnag/js": "^7.1.1",
    "@bugsnag/plugin-vue": "^7.1.1",
    "axios": "^0.27.2",
    "chart.js": "3.9.1",
    "chartjs-adapter-moment": "^1.0.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "clipboard-copy": "^3.1.0",
    "d3": "^7.6.1",
    "date-fns": "^2.29.3",
    "email-validator": "^2.0.4",
    "fast-xml-parser": "4.2.4",
    "file-system": "^2.2.2",
    "firebase": "9.20.0",
    "jquery": "^3.4.1",
    "jwt-decode": "^2.2.0",
    "libphonenumber-js": "^1.7.19",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "moment": "^2.24.0",
    "node-gyp": "^9.3.1",
    "sanitize-html": "^2.7.0",
    "serialize-error": "^4.1.0",
    "stylelint": "^14.11.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-order": "^5.0.0",
    "v-dragged": "0.0.5",
    "vue": "^3.1.5",
    "vue-async-computed": "^3.9.0",
    "vue-chartjs": "4.1.2",
    "vue-google-login": "^2.0.5",
    "vue-i18n": "^9.2.2",
    "vue-password-strength-meter": "^1.7.2",
    "vue-property-decorator": "^8.3.0",
    "vue-the-mask": "^0.11.1",
    "vue3-password-strength-meter": "^1.7.9",
    "vuedraggable": "^4.0.1",
    "vuetify": "^3.3.7",
    "web-animations-js": "^2.3.2",
    "webpack-plugin-vuetify": "^2.0.1",
    "zxcvbn": "^4.4.2"
  },
  "resolutions": {
    "node-sass": "npm:sass@1.32.6",
    "sass-loader": "9.0.3"
  },
  "exclusions": {
    "mdns2": "any"
  }
}
